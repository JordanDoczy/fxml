<?xml version="1.0" encoding="UTF-8"?>
<data>

	<!-- CHILDREN -->
	<addChild>
		<child class="org.fxml.Application">
			<constructor>
				<configFile>config/header.xml</configFile>
			</constructor>
			<name>header</name>
			<title><![CDATA[<body><span class="title">Examples FXML.</span></body>]]></title>
		</child>
	</addChild>
	
	<header instance="header">
		<addEventListener>
			<type reference="flash.events.Event.COMPLETE" />
			<listener instance="this.resume" />
		</addEventListener>
	</header>
	
	<pause />
	
	<!-- Body Copy -->
	<addChild>
		<child class="flash.text.TextField">
			<y>230</y>
			<x>10</x>
			<embedFonts>true</embedFonts>
			<autoSize reference="flash.text.TextFieldAutoSize.LEFT" />
			<antiAliasType reference="flash.text.AntiAliasType.ADVANCED" />
			<multiline>true</multiline>
			<selectable>false</selectable>
			<sharpness>50</sharpness>
			<thickness>50</thickness>
			<wordWrap>true</wordWrap>
			<width>550</width>
			<condenseWhite>true</condenseWhite>
			<styleSheet instance="this.header.bulkLoader.styleSheet"/>
			<htmlText>
				<![CDATA[
					<body>
					Lesson 1. <a href="Lesson1.html" target="_blank">Getting Started.</a><br/>
					Lesson 2. <a href="Lesson2.html" target="_blank">The Basics.</a><br/>
					Lesson 3. <a href="Lesson3.html" target="_blank">The Application.</a><br/>
					Lesson 4. <a href="Lesson4.html" target="_blank">The Instance Keyword.</a><br/>
					Lesson 5. <a href="Lesson5.html" target="_blank">The Function Keyword.</a><br/>
					Lesson 6. <a href="Lesson6.html" target="_blank">More Keywords.</a><br/>
					Lesson 7. <a href="Lesson7.html" target="_blank">Extending FXML.</a><br/>
					Lesson 8. <a href="Lesson8.html" target="_blank">The BulkLoader.</a><br/>
					Lesson 9. <a href="Lesson9.html" target="_blank">Fonts.</a><br/>
					</body>
				]]>
			</htmlText>
		</child>
	</addChild>
</data>
